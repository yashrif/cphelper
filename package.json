{
  "name": "cphelper",
  "version": "1.0.0",
  "description": "An Electron application for Competitive Programming",
  "main": "./out/main/index.js",
  "author": "Yashrif",
  "homepage": ".",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "migrate": "cd prisma && npx prisma migrate dev --name init"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.1",
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@prisma/client": "^4.10.1",
    "chakra-react-select": "^4.5.0",
    "color-thief-ts": "^1.1.1",
    "electron-updater": "^5.3.0",
    "framer-motion": "^10.0.0",
    "kute.js": "^2.2.4",
    "react-icons": "^4.7.1",
    "react-loading-skeleton": "^3.1.1",
    "recharts": "^2.4.3"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/node": "16.18.11",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-react": "^3.0.1",
    "axios": "^1.3.4",
    "electron": "^21.3.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^1.0.17",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "lodash": "^4.17.21",
    "prettier": "^2.8.2",
    "prisma": "^4.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "redux-logger": "^3.0.6",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "build": {
    "appId": "com.yashrif.cphelper",
    "productName": "CPHelper 1.0.0",
    "extraResources": [
      "prisma/**/*",
      "node_modules/.prisma/**/*",
      "node_modules/@prisma/client/**/*"
    ],
    "extraMetadata": {
      "main": "out/main/index.js"
    }
  }
}
