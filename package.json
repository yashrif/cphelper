{
  "name": "cphelper",
  "version": "1.0.0",
  "description": "An Electron application for Competitive Programming",
  "main": "./out/main/index.js",
  "author": "Yashrif",
  "homepage": ".",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "migrate": "cd prisma && npx prisma migrate dev --name init",
    "publish": "npm run build && electron-builder --win --config -p always"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.5",
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@prisma/client": "^4.12.0",
    "chakra-react-select": "^4.6.0",
    "color-thief-ts": "^1.1.1",
    "electron-updater": "^5.3.0",
    "framer-motion": "^10.10.0",
    "kute.js": "^2.2.4",
    "react-icons": "^4.8.0",
    "react-loading-skeleton": "^3.2.0",
    "recharts": "^2.5.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/node": "18.15.11",
    "@types/react": "18.0.32",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^3.1.0",
    "axios": "^1.3.4",
    "electron": "^23.2.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^1.0.21",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "lodash": "^4.17.21",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "redux-logger": "^3.0.6",
    "typescript": "^5.0.3",
    "vite": "^4.2.1"
  }
}
